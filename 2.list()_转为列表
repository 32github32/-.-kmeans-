my_string = "hello"
my_list = list(my_string)
print(my_list)  # 输出: ['h', 'e', 'l', 'l', 'o']


my_tuple = (1, 2, 3)
my_list = list(my_tuple)
print(my_list)  # 输出: [1, 2, 3]

my_set = {4, 5, 6}
my_list = list(my_set)
print(my_list)  # 输出: [4, 5, 6]


original_list = [1, 2, 3]
new_list = original_list[:]
print(new_list)  # 输出: [1, 2, 3]
